{
  "name": "byteschool",
  "version": "1.0.0",
  "description": "ByteSchool - Learn programming with interactive tutorials for CoffeeScript, Python, and more",
  "engines": {
    "node": ">=18.0.0 <23.0.0"
  },
  "scripts": {
    "dev": "parcel index.html --port 3001",
    "dev:open": "parcel index.html --open --port 3001",
    "build": "parcel build index.html --public-url ./",
    "build:gh": "parcel build index.html --public-url /byteschool/",
    "clean": "rm -rf dist .parcel-cache",
    "lint": "coffeelint src/",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@parcel/transformer-coffeescript": "^2.12.0",
    "@parcel/transformer-sass": "^2.12.0",
    "@types/node": "^20.0.0",
    "coffeelint": "^2.1.0",
    "coffeescript": "^2.7.0",
    "marked": "^17.0.1",
    "parcel": "^2.12.0",
    "sass": "^1.69.0",
    "typescript": "^5.3.0",
    "uuid": "^13.0.0"
  }
}
